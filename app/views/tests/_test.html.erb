<table>
  <tr>
    <th>Title</th>
    <th>Level</th>
    <th>Questions</th>
    <th>Actions</th>
  </tr>
  <tr>
    <td><%= test.title %></td>
    <td><%= test.level %>
    <td><%= test.questions.count %></td>
    <td><%= link_to 'Show', test_path(test) %>
        <%= link_to 'Edit', edit_test_path(test) %>
        <%= link_to 'Delete', test_path(test),
                            method: :delete,
                            data: { confirm: 'Are you sure?' }%></td>
  </tr>
</table>
